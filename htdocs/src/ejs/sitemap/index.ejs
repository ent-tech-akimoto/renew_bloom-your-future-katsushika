<%
var pageID = "sitemap";
var lang = "ja";
var relative_path = "../";
var myMeta = meta[lang][pageID];
%>
<!DOCTYPE html>
<html lang="<%= lang %>">
  <%-include(`${relative_path}_partial/_head`,{lang:lang,pageID:pageID,relative_path:relative_path})%>

  <body>
    <div class="page">
      <%-include(`${relative_path}_partial/_header`,{lang:lang,pageID:pageID,relative_path:relative_path})%>
      <article class="sitemap__wrapper">
        <div class="sitemap__bg"></div>
        <p class="common__bread">
          <span
            ><a href="https://test-bloom-your-future-katsushika.entacl.net/"
              >TOP</a
            ></span
          ><span>サイトマップ</span>
        </p>
        <h1 class="common__h1 sitemap__h1">サイトマップ</h1>
        <section class="sitemap__section">
          <div class="sitemap__column">
            <div class="sitemap__box">
              <a href="<%= relative_path %>overview" class="main">開催概要</a>
              <a href="<%= relative_path %>overview/#anchor-about">
                「全国みどりと花のフェアかつしか」について
              </a>
              <a href="<%= relative_path %>overview/#anchor-plan"> 基本計画 </a>
              <a href="<%= relative_path %>overview/#anchor-action">
                実施計画
              </a>
              <a href="<%= relative_path %>overview/#anchor-amba">
                アンバサダー
              </a>
              <a href="<%= relative_path %>overview/#anchor-logo">
                ロゴマーク
              </a>
            </div>
            <div class="sitemap__box">
              <a href="<%= relative_path %>area" class="main">開催エリア</a>
            </div>
            <div class="sitemap__box">
              <a href="<%= relative_path %>event" class="main">イベント情報</a>
              <a href="<%= relative_path %>event/calendar">
                イベントカレンダー
              </a>
            </div>
            <div class="sitemap__box">
              <a href="<%= relative_path %>sponsors" class="main">募集情報</a>
              <a href="<%= relative_path %>sponsors/#anchor-individual">
                個人向け</a
              >
              <a href="<%= relative_path %>sponsors/#anchor-group">
                企業・団体向け</a
              >
            </div>
            <div class="sitemap__box">
              <a href="<%= relative_path %>support" class="main">協賛募集</a>
            </div>
          </div>
          <div class="sitemap__box sitemap__column">
            <a href="<%= relative_path %>access" class="main">アクセス情報</a>
            <a href="<%= relative_path %>news" class="main">お知らせ</a>
            <a href="<%= relative_path %>related" class="main">関連サイト</a>
            <a href="" class="main">お問い合わせ</a>
          </div>
        </section>
      </article>
      <%-include(`${relative_path}_partial/_footer`,{lang:lang,pageID:pageID,relative_path:relative_path})%>
    </div>
    <%- include( `${relative_path}_partial/_foot`,{lang:lang,pageID:pageID,relative_path:relative_path}) %>
  </body>
</html>
